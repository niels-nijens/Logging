{
    "name": "niels-nijens/logging",
    "type": "library",
    "description": "A PSR-3 compatible logging library for PHP 5.3+",
    "keywords": [
        "PSR-3",
        "Logging",
        "Sentry"
    ],
    "homepage": "https://github.com/niels-nijens/Logging",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "nijens.niels@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "psr/log": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "raven/raven": "~0.7"
    },
    "suggest": {
        "raven/raven": "Required for usage of SentryLogAdapter."
    },
    "autoload": {
        "psr-4": {
            "Nijens\\Logging\\": "src/",
            "Nijens\\Logging\\Tests\\": "tests/"
        }
    }
}
